{
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ],
  "redirects": [
    {
      "source": "/home",
      "destination": "/",
      "permanent": true
    },
    {
      "source": "/shipping",
      "destination": "/shipping-policy",
      "permanent": true
    },
    {
      "source": "/terms",
      "destination": "/terms-and-conditions",
      "permanent": true
    },
    {
      "source": "/terms-of-service",
      "destination": "/terms-and-conditions",
      "permanent": true
    },
    {
      "source": "/contact",
      "destination": "/contact-us",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "index, follow"
        }
      ]
    }
  ]
}
